# 阿里云 DashScope API Key（北京地域）
DASHSCOPE_API_KEY=sk-your-api-key-here

# API 端点
NEXT_PUBLIC_API_BASE_URL=https://dashscope.aliyuncs.com

# 文件上传限制（字节）
NEXT_PUBLIC_MAX_IMAGE_SIZE=10485760    # 10MB
NEXT_PUBLIC_MAX_VIDEO_SIZE=104857600   # 100MB

# 轮询配置
NEXT_PUBLIC_POLL_INTERVAL=3000         # 3秒
NEXT_PUBLIC_POLL_TIMEOUT=300000        # 5分钟
